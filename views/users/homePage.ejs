                      <li>
                        <a href="product-left-sidebar.html" class="ms-pro-img"><img
                                src="/assets/img/uploads/<%=wishListItems[i].productId.images[0]%>" alt="product"></a>
                        <div class="ms-pro-content">
                            <a href="product-left-sidebar.html" class="wish-pro-title"><%=wishListItems[i].productId.productName%></a>
                            <span class="wish-price"><span>$<%=wishListItems[i].productId.price%></span> x 1 kg</span>
                            <div class="ms-availability">
                              <% if(wishListItems[i].productId.isListed){%>
                                <p class="stock avail">In Stock</p>
                                <% }else{ %>
                                  <p class="stock avail">Out Of Stock</p>
                                <% } %>
                                <a onclick="itemToCart('<%=wishListItems[i].productId%>')" class="ms-btn-2">Add to cart</a>
                            </div>
                            <a href="javascript:void(0)" onclick="wishlistRemover('<%=wishListItems[i]._id%>')" class="remove">×</a>
                        </div>
                    </li>
                    <% } %>
                  </ul>
              </div>
          </div>
