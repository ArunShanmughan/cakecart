        color: #333;
      }

      .order-status {
        font-size: 14px;
        font-weight: bold;
        color: #666;
        text-transform: uppercase;
      }

      .order-status.pending {
        color: #ffc107;
      }

      .order-status.completed {
        color: #28a745;
      }

      .pay-button {
        position: absolute;
        top: 50%;
        right: 20px;
        transform: translateY(-50%);
        background-color: #337ab7;
        border: none;
        color: #ffffff;
        padding: 10px 20px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 16px;
        margin: 4px 2px;
        cursor: pointer;
        border-radius: 5px;
        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
        transition: background-color 0.3s ease; /* added transition for hover effect */
      }

      .pay-button:hover {
        background-color: #23527c;
        box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
      }

      .pay-button a {
        color: #ffffff;
        text-decoration: none;
      }

      .pay-button a:hover {
        text-decoration: underline;
      }

      /* Order Summary styles */

      #order-summary {
        background-color: #f2f2f2;
        padding: 10px;
        border-bottom: 1px solid #ccc;
      }

      #order-summary p {
        margin-bottom: 10px;
      }

      /* Order Items styles */

      #order-items {
        padding: 10px;
        border-bottom: 1px solid #ccc;
      }

      #order-items p {
        margin-bottom: 10px;
      }

      /* Order Date styles */

      #order-date {
        padding: 10px;
        border-bottom: 1px solid #ccc;
      }

      #order-date p {
        margin-bottom: 10px;
      }

      /* Shipping Address styles */

      #shipping-address {
        padding: 10px;
      }

      #shipping-address p {
        margin-bottom: 10px;
      }

      /* Strong text styles */

      strong {
        font-weight: bold;
        color: #666;
      }

      /* Order Status styles */

      .order-status {
        font-weight: bold;
      }

      .order-status.pending {
        color: #ffc107; /* Yellow for pending */
      }

      .order-status.shipped {
        color: #28a745; /* Green for shipped */
      }

      .order-status.delivered {
        color: #007bff; /* Blue for delivered */
      }

      .order-status.cancelled {
        color: #dc3545; /* Red for cancelled */
      }

      .pay-button {
        background-color: #1a53ff;
        border: none;
        color: #ffffff;
        padding: 10px 20px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 16px;
        margin: 4px 2px;
        cursor: pointer;
        border-radius: 5px;
        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
      }

      .pay-button:hover {
        background-color: #0033cc;
        box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
      }

      .pay-button a {
        color: #ffffff;
        text-decoration: none;
      }

      .pay-button a:hover {
        text-decoration: underline;
      }
    </style>
    <script>
      let orderedDate = new Date(`<%= orderInfo.orderDate %>`);
      let currentDate = new Date();
      console.log(currentDate.getTime());
      console.log(orderedDate.getTime());
      var timeDifference = Math.floor(
        (currentDate.getTime() - orderedDate.getTime()) / (1000 * 3600 * 24)
      );
      console.log(timeDifference);

      if (
        `<%= orderInfo.orderStatus %>` == "delivered" &&
        timeDifference <= 14
      ) {
        var button = document.createElement("button");
        button.className = "pay-button btn-primary";
        button.innerHTML = `<a href="">return</a>`;
        document.getElementById("buttonContainer").appendChild(button); // append to a specific container
      }
    </script>

