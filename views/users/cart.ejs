    <div class="ms-cart-content">
      <div class="ms-cart-inner">
        <div class="row">
          <form action="#">
            <div class="table-content cart-table-content">
              <table>
                <thead>
                  <tr>
                    <th>Product</th>
                    <th>Price</th>
                    <th style="text-align: center">Quantity</th>
                    <th>Total</th>
                    <th></th>
                  </tr>
                </thead>
                <tbody>
                  <% userCartData.forEach((items,index) => { %>
                  <tr>
                    <td data-label="Product" class="ms-cart-pro-name">
                      <a href="product-left-sidebar.html">
                        <img
                          class="ms-cart-pro-img mr-4"
                          src="/assets/img/uploads/<%=items?.productId?.images[0]%>"
                          alt=""
                        /><%=items?.productId?.productName%>
                      </a>
                    </td>
                    <td data-label="Price" class="ms-cart-pro-price">
                      <span class="amount">₹<%=items?.productId?.price %></span>
                    </td>
                    <td
                      data-label="Quantity"
                      class="ms-cart-pro-qty"
                      style="text-align: center"
                    >
                      <style>
                        .product-quantity-container {
                          display: flex;
                          flex-direction: column;
                          align-items: center;
                          gap: 0.5rem;
                        }

                        .product-quantity-btn {
                          background-color: transparent;
                          border: none;
                          cursor: pointer;
                          font-size: 1.5rem;
                          transition: color 0.2s;
                        }

                        .product-quantity-minus-btn {
                          color: #dc3545;
                        }

                        .product-quantity-minus-btn:hover {
                          color: #c82333;
                        }

                        .product-quantity-plus-btn {
                          color: #28a745;
                        }

                        .product-quantity-plus-btn:hover {
                          color: #1e7e34;
                        }

                        .product-quantity-value {
                          font-size: 1.2rem;
                          font-weight: bold;
                        }
                      </style>
                      <div class="product-quantity-container">
                        <button
                          type="button"
                          class="product-quantity-btn product-quantity-minus-btn"
                          onclick="cartDec('<%=items._id %>','<%=index%>')"
                        >
                          <i class="fa-solid fa-minus"></i>
                        </button>
                        <span
                          id="productQuantity<%=index%>"
                          class="product-quantity-value"
                        >
                          &nbsp; &nbsp;<%= items?.productQuantity%> &nbsp;
                          &nbsp;
                        </span>
                        <button
                          type="button"
                          class="product-quantity-btn product-quantity-plus-btn"
                          onclick="cartInc('<%=items._id %>','<%=index %>')"
                        >
                          <i class="fa-solid fa-plus"></i>
                        </button>
                      </div>
                    </td>
                    <td
                      data-label="Total"
                      class="ms-cart-pro-subtotal"
                      id="totalCostPerProduct<%=index%>"
                    >
                      ₹<%=items.totalCostPerProduct%>
                    </td>
                    <td data-label="Remove" class="ms-cart-pro-remove">
                      <button onclick="deleteItem('<%=items._id%>')">
                        <i class="msicon msi-trash-o"></i>
                      </button>
                    </td>
                  </tr>
                  <script>
                    async function cartDec(id, index) {
                      try {
                        console.log("decreasing function is calling...");
                        let productQuantityCart = document.getElementById(
                          "productQuantity" + index
                        );
                        let totalCostPerProductElement =
                          document.getElementById(
                            "totalCostPerProduct" + index
                          );
                        let grandTotalOfCart =
                          document.getElementById("grandTotalAmount");

                        if (Number(productQuantityCart.innerText) == 1) {
                          return Swal.fire({
                            title: "Something Went Wrong",
                            text: "Your minimum quantity is reached",
                            icon: "warning",
                          });
                        }
                        let response = await fetch("/cart/decQty/" + id, {
                          method: "PUT",
                        });
                        let result = await response.json();
                        console.log(result);

                        productQuantityCart.innerText =
                          result.cartFindData.productQuantity;
                        totalCostPerProductElement.innerText =
                          "₹" + result.cartFindData.totalCostPerProduct;
                        grandTotalOfCart.innerText =
                          "₹"+result.wholeTotal;
                        // window.location.reload();
                      } catch (error) {
                        console.log("Something Went Wrong", error);
                      }
                    }

                    async function cartInc(id, index) {
                      try {
                        let productQuantityCart = document.getElementById(
                          "productQuantity" + index
                        );
                        let totalCostPerProductElement =
                          document.getElementById(
                            "totalCostPerProduct" + index
                          );
                        let grandTotalOfCart =
                          document.getElementById("grandTotalAmount");
                        console.log("this is the grandtotalofCart",grandTotalOfCart);
                        if (
                          Number(productQuantityCart.innerText) >=
                          "<%=items?.productId?.quantity%>"
                        ) {
                          console.log(
                            "This increasing quantity condition is true"
                          );
                          return Swal.fire({
                            title: "Oops...",
                            text: "Maximum qunatity reached!.",
                            icon: "warning",
                          });
                        }
                        let response = await fetch("/cart/incQty/" + id, {
                          method: "PUT",
                        });
                        let result = await response.json();
                        console.log(result);
                        productQuantityCart.innerText =
                          result.cartFindData.productQuantity;
                        totalCostPerProductElement.innerText =
                          "₹" + result.cartFindData.totalCostPerProduct;
                        grandTotalOfCart.innerText =
                          "₹"+result.wholeTotal;
                        // window.location.reload();
                      } catch (error) {
                        console.log("Something Went wrong", error);
                      }
                    }

                    async function deleteItem(id) {
                      console.log("coming to the delete function");
                      try {
                        await Swal.fire({
                          title: "Are you sure?",
                          text: "You want to delete this!",
                          icon: "warning",
                          showCancelButton: true,
                          confirmButtonColor: "#3085d6",
                          cancelButtonColor: "#d33",
                          confirmButtonText: "Yes, delete it!",
                        }).then(async (result) => {
                          if (result.isConfirmed) {
                            let response = await fetch(`/deleteCart/${id}`, {
                              method: "POST",
                              headers: { "Content-Type": "application/json" },
                            });
                            let res = await response.json();
                            console.log(res);
                            if (res.success) {
                              Swal.fire({
                                title: "Deleted!",
                                text: "Your file has been deleted.",
                                icon: "success",
                                confirmButtonColor: "#3085d6",
                                cancelButtonColor: "#d33",
                                confirmButtonText: "Successfully deleted ",
                              }).then((result) => {
                                if (result.isConfirmed)
                                  window.location.reload();
                              });
                            }
                          }
                        });

                        // console.log(result);
                      } catch (error) {
                        console.log("something Went Wrong", error);
                      }
                    }
                  </script>
                  <% }) %>
                </tbody>
              </table>
            </div>
            <div class="grand-total-box">
              <p style="font-weight: bold">Grand Total:</p>
              <p id="grandTotalAmount">₹:<%=wholeTotal%></p>
            </div>

