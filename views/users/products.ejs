                    name="ms-select"
                    id="ms-sort"
                    onchange="sortProducts(event)"
                    class="form-control"
                  >
                    <option selected disabled>Sort by</option>
                    <option value="1">Name, A to Z</option>
                    <option value="2">Name, Z to A</option>
                    <option value="3">Price, low to high</option>
                    <option value="4">Price, high to low</option>
                    <option value="5">New Arrivals</option>
                  </select>
                </div>
              </div>
              <script>
                async function sortProducts(e) {
                  let response = await fetch(
                    `/products/sort/sortValue?sortId=${e.target.value}`,
                    {
                      method: "GET",
                    }
                  );
                  const result = await response.json();
                  if (result.ascSort) {
                    window.location.reload();
                  } else if (result.desSort) {
                    window.location.reload();
                  } else if (result.highValueSort) {
                    window.location.reload();
                  } else if (result.lowValueSort) {
                    window.location.reload();
                  } else if (result.newArrive) {
                    window.location.reload();
                  }
                }
              </script>
