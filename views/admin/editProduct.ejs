         <script>
           function previewImages(event) {
    const previewContainer = document.getElementById('new-image-preview');
    previewContainer.innerHTML = '';

    const files = event.target.files;

    if (files) {
      Array.from(files).forEach((file, index) => {
        const reader = new FileReader();

        reader.onload = function(e) {
          const imgElement = document.createElement('div');
          imgElement.classList.add('image-item');
          imgElement.style.position = 'relative';
          imgElement.style.display = 'inline-block';
          imgElement.style.margin = '5px';
          imgElement.innerHTML = `
            <img src="${e.target.result}" class="image-preview">
            <button class="delete-btn" type="button" onclick="deleteNewImage(${index})">Delete</button>
            <button class="crop-btn" type="button" onclick="cropNewImage(${index})">Crop</button>
          `;
          previewContainer.appendChild(imgElement);
        };

        reader.readAsDataURL(file);
      });
    }
  }

    function deleteNewImage(index) {
      const previewContainer = document.getElementById('new-image-preview');
      const images = previewContainer.getElementsByClassName('image-item');
      if (images[index]) {
        previewContainer.removeChild(images[index]);

        const inputField = document.getElementById('images');
        const files = Array.from(inputField.files);
        files.splice(index, 1);
        inputField.files = new DataTransfer().files = files;

        
        inputField.value = ''; 
        inputField.files = new DataTransfer().files = files; 
        inputField.dispatchEvent(new Event('change')); 
      }
    }

  function cropNewImage(index) {
    const previewContainer = document.getElementById('new-image-preview');
    const images = previewContainer.getElementsByClassName('image-item');
    const imageSrc = images[index].getElementsByTagName('img')[0].src;
    const cropperModal = document.createElement('div');
    cropperModal.innerHTML = `
      <div style="display: flex; justify-content: center; align-items: center; background: rgba(0,0,0,0.8); position: fixed; top: 0; left: 0; width: 100%; height: 100%;">
        <div style="background: white; padding: 10px;">
          <img id="cropper-image" src="${imageSrc}" style="max-width: 100%; max-height: 80vh;">
          <button onclick="confirmCrop(${index})">Crop</button>
          <button onclick="closeCropper()">Cancel</button>
        </div>
      </div>`;
    document.body.appendChild(cropperModal);
    const image = document.getElementById('cropper-image');
    const cropper = new Cropper(image, {
      aspectRatio: 1,
      viewMode: 1,
    });

    window.confirmCrop = function(idx) {
      const canvas = cropper.getCroppedCanvas();
      const croppedImageDataURL = canvas.toDataURL('image/jpeg');
      // Update the preview image with the cropped image
      const previewImage = previewContainer.getElementsByClassName('image-item')[idx].getElementsByTagName('img')[0];
      previewImage.src = croppedImageDataURL;
      closeCropper();
    }

    window.closeCropper = function() {
      cropper.destroy();
      cropperModal.remove();
    }
  }
        
        
          function handleImagePreview(files) {
            const previewContainer = document.getElementById('image-preview-container');
            previewContainer.innerHTML = '';
        
            for (let i = 0; i < files.length; i++) {
              const file = files[i];
              const reader = new FileReader();
        
              reader.onload = function (e) {
                const imageItem = document.createElement('div');
                imageItem.className = 'image-item';
                imageItem.style.cssText = 'display: inline-block; position: relative; margin: 5px;';
                imageItem.innerHTML = `
                  <img src="${e.target.result}" style="height: 60px; width: 60px; border-radius: 0;">
                  <button class="delete-btn" type="button" onclick="removePreviewImage(${i})" style="position: absolute; top: 0; right: 0;">Delete</button>`;
                previewContainer.appendChild(imageItem);
              };
        
              reader.readAsDataURL(file);
            }
          }

          async function deleteImage(imageIndex, productId) {
           try {
             const response = await fetch('/delete-image', {
               method: 'POST',
               headers: {
                 'Content-Type': 'application/json'
               },
               body: JSON.stringify({ imageIndex, productId })
             });

             const result = await response.json();

             if (result.success) {
               Swal.fire({
                 icon: 'success',
                 title: 'Success',
                 text: 'Image deleted successfully'
               });
               // Optionally, you can remove the image from the DOM here
               document.querySelector(`.image-item:nth-child(${imageIndex + 1})`).remove();
             } else {
               Swal.fire({
                 icon: 'error',
                 title: 'Error',
                 text: 'Image could not be deleted'
               });
             }
           } catch (error) {
             Swal.fire({
               icon: 'error',
               title: 'Error',
               text: 'An error occurred'
             });
           }
         }
        
          function removePreviewImage(index) {
            const previewContainer = document.getElementById('image-preview-container');
            const previewImages = previewContainer.getElementsByClassName('image-item');
        
            if (previewImages.length > index) {
              previewContainer.removeChild(previewImages[index]);
            }
          }
        
          document.getElementById('images').addEventListener('change', function () {
            handleImagePreview(this.files);
          });
        
          $.validator.addMethod("startspace", function (value, element) {
            return value.trim().charAt(0) !== " ";
          }, "Product name should not start with a space.");
        
          $.validator.addMethod("noMoreThanTwoSpaces", function (value, element) {
            return value.trim().replace(/\s+/g, " ").match(/ {2,}/) === null;
          }, "Product name should not have more than two spaces together.");
        
          $("#editproduct").validate({
            rules: {
              productName: {
                required: true,
                minlength: 2,
                maxlength: 50,
                startspace: true,
                noMoreThanTwoSpaces: true,
              },
              productPrice: {
                required: true,
                pattern: "[0-9]+",
              },
              productStock: {
                required: true,
                pattern: "[0-9]+",
              },
              parentCategory: {
                required: true,
              },
              images: {
                required: function() {
                  console.log("This validation is working inside of the image validation")
                  return productImages.length + document.getElementById('images').files.length < 3;
                }
              }
            },
            messages: {
              images: {
                required: "At least 3 images are required.",
              }
            },
            submitHandler: async (form, event) => {
              event.preventDefault();
              // Create a new FormData object
              const formData = new FormData(form);
        
              const fileInputs = form.querySelectorAll('input[type="file"][name="images"]');
              fileInputs.forEach((input) => {
                const files = input.files;
                for (let i = 0; i < files.length; i++) {
                  formData.append("images", files[i]);
                }
              });
        
              if (productImages.length + document.getElementById('images').files.length < 3) {
                await Swal.fire({
                  icon: "error",
                  title: "At least 3 images are required.",
                });
                return;
              }

              console.log("coming to the fetch method");
              let response = await fetch("/editProducts/<%=productInfo?._id%>", {
                method: "POST",
                body: formData,
              });
              console.log("it's inside the fetchMethod")
              let result = await response.json();
              console.log("this is from the back end result in save changes in edit product",result)
              if (result.exists) {
                await Swal.fire({
                  icon: "error",
                  title: "Product Already exists",
                });
              }
              if (result.success) {
                await Swal.fire({
                  icon: "success",
                  title: "Product Added",
                }).then(()=>window.location.href = "/productManagment")
                
              }
              if (result.noValue) {
                await Swal.fire({
                  icon: "error",
                  title: "No Value in Product Name",
                });
              }
            },
          });
        </script>
     
        
