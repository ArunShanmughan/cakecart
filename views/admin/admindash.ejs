    <script>
      function applyFilter() {
        const filterValue = document.getElementById("filter-select").value;
        console.log("Filter value selected:", filterValue);
        fetchData(filterValue);
      }

      async function fetchData(filterData) {
        try {
          console.log("Fetching data with filter:", filterData);
          const response = await fetch(`/dashBoardData?filter=${filterData}`, {
            method: "GET",
          });
          const data = await response.json();
          console.log("Data fetched:", data);
          // Update the charts with the new data
          updateCharts(data);
        } catch (error) {
          console.error("Error fetching data:", error);
        }
      }

      function updateCharts(data) {
        const {
          productCount,
          categoryCount,
          pendingOrders,
          completedOrdersCount,
          currentDayRevenue,
          fourteenDayRevenue,
          categoryWiseRevenue,
          totalRevenue,
          monthlyRevenue,
          activeUsers,
        } = data;

        console.log("Updating charts with data:", data);

        // Update the text elements
        document.querySelector(
          ".total-revenue-count"
        ).innerText = `${totalRevenue.revenue}`;
        document.querySelector(
          ".Total-Sales-count"
        ).innerText = `${completedOrdersCount}`;
        document.querySelector(
          ".Current-Day-count"
        ).innerText = `${currentDayRevenue}`;
        document.querySelector(
          ".Active-user-count"
        ).innerText = `${activeUsers}`;

        // Update the charts
        const fourteenDayChartElement = document.getElementById("TrafficChart");
        console.log("Fourteen Day Chart Element:", fourteenDayChartElement);

        const fourteenDayChartOption = {
          type: "radar",
          data: {
            labels: fourteenDayRevenue.date,
            datasets: [
              {
                label: "Last 2 Weeks Revenue per Day",
                data: fourteenDayRevenue.revenue,
                borderWidth: 1,
              },
            ],
          },
          options: {
            title: {
              display: true,
              text: "Revenue for the Last 2 Weeks",
            },
          },
        };

        const categoryRevenueChartElement =
          document.getElementById("dougnutChart");
        console.log(
          "Category Revenue Chart Element:",
          categoryRevenueChartElement
        );

        const randomColors = getRandomColors(
          categoryWiseRevenue.categoryName.length
        );
        const categoryRevenueChartOptions = {
          type: "doughnut",
          data: {
            labels: categoryWiseRevenue.categoryName,
            datasets: [
              {
                label: "Category wise Revenue in dollars",
                backgroundColor: randomColors,
                data: categoryWiseRevenue.revenuePerCategory,
              },
            ],
          },
        };

        // Ensure the charts are properly destroyed before creating new ones
        if (fourteenDayChartElement.chart) {
          fourteenDayChartElement.chart.destroy();
        }
        if (categoryRevenueChartElement.chart) {
          categoryRevenueChartElement.chart.destroy();
        }

        // Create new charts
        console.log("Creating new Fourteen Day Chart...",fourteenDayChartOption);
        fourteenDayChartElement.chart = new Chart(
          fourteenDayChartElement,
          fourteenDayChartOption
        );

        console.log("Creating new Category Revenue Chart...");
        categoryRevenueChartElement.chart = new Chart(
          categoryRevenueChartElement,
          categoryRevenueChartOptions
        );
      }

      // Call the fetchData function initially with the default filter value
      console.log("Initial data fetch with default filter value...");
      fetchData("Two Week");

      // Helper function to generate random colors
      function getRandomColors(length) {
        return Array.from(
          { length },
          () => `#${Math.floor(Math.random() * 16777215).toString(16)}`
        );
      }
    </script>
  </body>
</html>
