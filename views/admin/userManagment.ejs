      <div class="container">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Names</th>
              <th scope="col">Email</th>
              <th scope="col">Mobile</th>
              <th scope="col">Block/Unblock</th>
            </tr>
          </thead>
          <tbody>
            <% userinfo.forEach(x => { %>
            <tr>
              <th scope="row"><%=x.fName%></th>
              <td><%= x.email %></td>
              <td><%= x.mobile %></td>
              <% if(x.isBlocked){ %>
              <td>
                <button
                  id="unblock<%x%>"
                  onclick="fnBlock('<%=x._id %>')"
                  type="button"
                  class="btn btn-success"
                >
                  Unblock
                </button>
              </td>
              <% }else{ %>
              <td>
                <button
                  id="block<%x%>"
                  onclick="fnUnBlock('<%=x._id %>')"
                  type="button"
                  class="btn btn-danger"
                >
                  Block
                </button>
              </td>
              <% } %>
            </tr>
            <% }); %>
