  <script>
    async function fnBlock(userId) {
      let response = await fetch(`/userBlock?id=${userId}`, {
        method: "GET",
      });
      let result = await response.json();
      if (result.success) {
        await Swal.fire({
          icon: "success",
          title: "User unblocked",
        });
      }
      window.location.reload();
    }

    async function fnUnBlock(userId) {
      let response = await fetch(`/userUnBlock?id=${userId}`, {
        method: "GET",
      });
      let result = await response.json();
      if (result.success) {
        await Swal.fire({
          icon: "error",
          title: "User blocked",
          confirmButtonText: "OK",
        });
      }
      window.location.reload();
    }
  </script>
