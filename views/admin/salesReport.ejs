          ],
          {
            series: {
              lines: {
                show: true,
                lineColor: "#fff",
                lineWidth: 2,
              },
              points: {
                show: true,
                fill: true,
                fillColor: "#ffffff",
                symbol: "circle",
                radius: 3,
              },
              shadowSize: 0,
