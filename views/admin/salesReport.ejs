      <body>
        <h1>Sales Report</h1>
        <form class="filter-container" id="date-filter">
          <% if(dateValues){ %>
          <label for="start-date">Start Date:</label>
          <input
            type="date"
            id="start-date"
            name="startDate"
            value="<%= dateValues.dateFrom%>"
          />
          <label for="end-date">End Date:</label>
          <input
            type="date"
            id="end-date"
            name="endDate"
            value="<%= dateValues.dateTo %>"
          />
          <% }else{ %>
          <label for="start-date">Start Date:</label>
          <input type="date" id="start-date" name="startDate" />
          <label for="end-date">End Date:</label>
          <input type="date" id="end-date" name="endDate" />
          <% } %>
          <button class="btn btn-primary" type="submit" id="apply-filters">
            Apply Filters
          </button>
          <button class="btn btn-secondary" type="button" id="remove-filters">
            Remove Filters
          </button>
        </form>
        <div class="download-buttons">
          <button class="btn btn-success" id="download-excel">
            Download Excel
          </button>
          <button type="button" class="btn btn-danger" id="download-pdf">
            Download PDF
          </button>
        </div>
        <table id="sales-report-table">
          <thead>
            <tr>
              <th>Order Number</th>
              <th>User Name</th>
              <th>Order Date</th>
              <th>Products</th>
              <th>Product Offer</th>
              <th>Quantity</th>
              <th>Before Offer</th>
              <th>Total Cost</th>
              <th>Payment Method</th>
              <th>Status</th>
              <th>Coupons Applied</th>
              <th>Before Coupon</th>
              <th>Order Price</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < salesData.length; i++) { %>
            <tr>
              <td><%= salesData[i].orderNumber %></td>
              <td><%= salesData[i].userId.fName %></td>
              <td>
                <%= new Date(salesData[i].orderDate).toLocaleDateString() %>
              </td>
              <td>
                <% salesData[i].cartData.forEach(product => { %> <%=
                product.productId.productName %>
                <hr />
                <% }) %>
              </td>
              <td>
                <% if (!salesData[i].productOffers) { %> Nill <% } else { %> <%=
                salesData[i].grandTotalcost %> <% } %>
              </td>
              <td>
                <% salesData[i].cartData.forEach(product => { %> <%=
                product.productQuantity %>
                <hr />
                <% }) %>
              </td>
              <td>
                <% salesData[i].cartData.forEach(product => { %> Rs.<%=
                product.productId.price %>
                <hr />
                <% }) %>
              </td>
              <td>Rs.<%= salesData[i].grandTotalcost %></td>
              <td><%= salesData[i].paymentType %></td>
              <td>Delivered</td>
              <td>
                <% if (salesData[i].length === 0) { %> None <% }
                else { %> <%= salesData[i].couponOffers.couponCode %> <% } %>
              </td>
              <td>
                <% if (salesData[i].couponOffers) { %>
                Rs.<%=Math.round(salesData[i].grandTotalcost / (1 -
                salesData[i].couponOffers.discountPercentage / 100)) %> <% }
                else { %> Rs.<%= salesData[i].grandTotalcost %> <% } %>
              </td>
              <td>Rs.<%= salesData[i].grandTotalcost %></td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </body>

