      <script>
        $("#date-filter").validate({
          submitHandler: async (form, event) => {
            event.preventDefault();
            let formData = {
              dateFrom: form.startDate.value,
              dateTo: form.endDate.value,
            };
            console.log("calling this date filter function", formData);
            try {
              let response = await fetch("/salesDateFilter", {
                method: "POST",
                headers: { "Content-Type": "application/json" },
                body: JSON.stringify(formData),
              });

              let result = await response.json();
              if (result.success) {
                console.log(
                  "this is because the data is being passed here success"
                );
                window.location.reload();
              }
            } catch (error) {
              console.log("Something went wrong with the filter", error);
            }
          },
        });
      </script>
