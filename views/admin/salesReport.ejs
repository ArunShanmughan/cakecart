        $.plot("#cellPaiChart", cellPaiChart, {
          series: {
            pie: {
              show: true,
              stroke: {
                width: 0,
              },
            },
          },
          legend: {
            show: false,
          },
          grid: {
            hoverable: true,
            clickable: true,
