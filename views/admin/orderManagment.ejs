      <script>
        async function orderStatus(e, ordId) {
          console.log("this is coming as e target value", e.target.value);
          let response = await fetch(
            `/orderStatusChange/${ordId}?statusId=${e.target.value}`
          );
          const result = await response.json();
          if (result.success) {
            window.location.reload();
          }
        }

